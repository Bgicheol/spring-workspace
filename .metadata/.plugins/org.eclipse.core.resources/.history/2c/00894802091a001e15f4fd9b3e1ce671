package com.ezen.springmvc.model;

import java.util.Date;

import lombok.Getter;
import lombok.Setter;
import lombok.ToString;


// 롬복이 Getter, Setter, 생성자, toString등을 모두 자동으로 생성해준다
@Getter
@ToString
public class EmployeeDTO {
	
	String first_name;
	@Setter
	String last_name;
	Date hire_data;
	Integer salary;
}
